{
  "name": "mimmi20/laminas-router-hostname",
  "description": "Provides a GitHub repository laminas-router-hostname for a PHP library, using GitHub actions.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/laminas-router-hostname",
  "support": {
    "issues": "https://github.com/mimmi20/laminas-router-hostname/issues",
    "source": "https://github.com/mimmi20/laminas-router-hostname"
  },
  "require": {
    "php": "^8.1.0",
    "ext-mbstring": "*",
    "laminas/laminas-router": "^3.11.1",
    "laminas/laminas-stdlib": "^3.17.0"
  },
  "require-dev": {
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.26.19",
    "laminas/laminas-modulemanager": "^2.14.0",
    "laminas/laminas-servicemanager": "^3.20.0",
    "mimmi20/coding-standard": "^4.0.5",
    "nikic/php-parser": "^v4.15.4",
    "phpstan/extension-installer": "^1.2.0",
    "phpstan/phpstan": "^1.10.13",
    "phpstan/phpstan-deprecation-rules": "^1.1.3",
    "phpstan/phpstan-phpunit": "^1.3.11",
    "phpunit/phpunit": "^10.0.19"
  },
  "suggest": {
    "laminas/laminas-modulemanager": "Required for Module Integration",
    "laminas/laminas-servicemanager": "Required for the Factories"
  },
  "autoload": {
    "psr-4": {
      "Mimmi20\\Routing\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\Routing\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "lock": false,
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "laminas/laminas-dependency-plugin": true,
      "phpstan/extension-installer": true,
      "infection/extension-installer": true
    }
  },
  "extra": {
    "laminas": {
      "component": "Mimmi20\\Routing",
      "config-provider": "Mimmi20\\Routing\\ConfigProvider"
    }
  }
}
