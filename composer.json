{
  "name": "mimmi20/laminas-router-hostname",
  "type": "library",
  "description": "Provides a GitHub repository laminas-router-hostname for a PHP library, using GitHub actions.",
  "homepage": "https://github.com/mimmi20/laminas-router-hostname",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4.3 || ^8.0.0",
    "laminas/laminas-router": "^3.5.0"
  },
  "require-dev": {
    "laminas/laminas-dependency-plugin": "^2.2.0",
    "laminas/laminas-modulemanager": "^2.11.0",
    "laminas/laminas-servicemanager": "^3.10.0",
    "mimmi20/coding-standard": "^2.5.1",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^1.2.0",
    "phpstan/phpstan-deprecation-rules": "^1.0.0",
    "phpstan/phpstan-phpunit": "^1.0.0",
    "phpunit/phpunit": "^9.5.10",
    "rector/rector": "^0.12.5"
  },
  "suggest": {
    "laminas/laminas-servicemanager": "Required for the Factories"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "component": "Mimmi20\\Routing",
      "config-provider": "Mimmi20\\Routing\\ConfigProvider"
    }
  },
  "autoload": {
    "psr-4": {
      "Mimmi20\\Routing\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\Routing\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/mimmi20/laminas-router-hostname/issues",
    "source": "https://github.com/mimmi20/laminas-router-hostname"
  }
}
